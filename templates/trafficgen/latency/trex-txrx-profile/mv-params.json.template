                { "arg": "teaching-measurement-packet-rate", "vals": [ "100" ], "role": "client" },
                { "arg": "teaching-measurement-interval", "vals": [ "10.0" ], "role": "client" },
                { "arg": "rate-tolerance", "vals": [ "5" ], "role": "client" },
                { "arg": "runtime-tolerance", "vals": [ "5" ], "role": "client" },
                { "arg": "rate-tolerance-failure", "vals": [ "fail" ], "role": "client" },
                { "arg": "max-loss-pct", "vals": [ "0.002" ], "role": "client" },
                { "arg": "disable-flow-cache", "vals": [ "OFF" ], "role": "client" },
                { "arg": "search-granularity", "vals": [ "0.05" ], "role": "client" },
                { "arg": "random-seed", "vals": [ "42" ], "role": "client" },
                { "arg": "disable-upward-search", "vals": [ "ON" ], "role": "client" }
            ]
        }
    ]
}
