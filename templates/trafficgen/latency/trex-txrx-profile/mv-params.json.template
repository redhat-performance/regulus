                { "arg": "trex-software-mode", "vals": [ "$TPL_SW_MODE" ], "role": "client", "enabled": "yes" },
                { "arg": "trex-mellanox-support", "vals": [ "$TPL_MLXSUP" ], "role": "client", "enabled": "yes" },
                { "arg": "rate-unit", "vals": [ "%" ], "role": "client" },
                { "arg": "min-rate", "vals": [ "1" ], "role": "client" },
                { "arg": "rate", "vals": [ "60" ], "role": "client" },
                { "arg": "latency-rate", "vals": [ "1000" ], "role": "client" },
                { "arg": "one-shot", "vals": [ "0" ], "role": "client" },
                { "arg": "search-runtime", "vals": [ "30" ], "role": "client" },
                { "arg": "validation-runtime", "vals": [ "300" ], "role": "client" },
                { "arg": "stream-mode", "vals": [ "continuous" ], "role": "client" },
