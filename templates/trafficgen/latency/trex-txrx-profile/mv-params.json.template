                { "arg": "testpmd-descriptors", "vals": [ "1024" ], "role": "server" },
                { "arg": "testpmd-burst", "vals": [ "32" ], "role": "server" },
                { "arg": "server-devices", "vals": [ "TWO_PCI_RSRC_NAMES" ], "role": "server", "enabled": "yes" }
            ]
        }
    ],
    "sets": [
        {
            "include": "global",
            "params": [
                { "arg": "trex-devices",        "vals": [ "TWO_PCI_ADDRS" ], "role": "client" },
                { "arg": "trex-active-devices", "vals": [ "TWO_PCI_ADDRS" ], "role": "client" },
