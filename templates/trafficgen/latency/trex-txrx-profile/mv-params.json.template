{
    "global-options": [
        {
            "name": "global",
            "params": [
                { "arg": "switch-type", "vals": [ "testpmd" ], "role": "server" },
                { "arg": "testpmd-forward-mode", "vals": [ "$TPL_FWD_MODE" ], "role": "server" },
                { "arg": "testpmd-enable-rss-udp", "vals": [ "on" ], "role": "server" },
                { "arg": "testpmd-queues", "vals": [ "1" ], "role": "server" },
                { "arg": "testpmd-queues-per-pmd", "vals": [ "1" ], "role": "server" },
                { "arg": "testpmd-smt", "vals": [ "on" ], "role": "server" },
