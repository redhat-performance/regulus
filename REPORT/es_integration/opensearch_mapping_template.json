{
  "index_patterns": ["regulus-*"],
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1,
      "index.mapping.total_fields.limit": 2000
    },
    "mappings": {
      "_meta": {
        "description": "Regulus benchmark results schema",
        "version": "1.0",
        "field_descriptions": {
          "cpu": "Number of CPUs allocated to one pod in Kubernetes resource (count, not percentage). Suffix '(Gu)' indicates guaranteed CPUs",
          "busy_cpu": "Sum of busy CPU count from both client and server worker nodes involved in the test (count, not percentage)",
          "execution_label": "Test execution label (e.g., dpu-accelerated, non-accelerated)",
          "regulus_data": "Source file path for the test result",
          "run_id": "Unique identifier for the test run",
          "iteration_id": "Unique identifier for the test iteration",
          "mean": "Mean throughput value",
          "stddev": "Standard deviation of throughput",
          "stddev_pct": "Standard deviation as percentage",
          "unit": "Unit of measurement for throughput (e.g., Gbps, transactions-sec)"
        }
      },
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "regulus_data": {
          "type": "keyword"
        },
        "run_id": {
          "type": "keyword"
        },
        "iteration_id": {
          "type": "keyword"
        },
        "benchmark": {
          "type": "keyword"
        },
        "test_type": {
          "type": "keyword"
        },
        "protocol": {
          "type": "keyword"
        },
        "model": {
          "type": "keyword"
        },
        "nic": {
          "type": "keyword"
        },
        "arch": {
          "type": "keyword"
        },
        "cpu": {
          "type": "keyword"
        },
        "kernel": {
          "type": "keyword"
        },
        "rcos": {
          "type": "keyword"
        },
        "topology": {
          "type": "keyword"
        },
        "performance_profile": {
          "type": "keyword"
        },
        "offload": {
          "type": "keyword"
        },
        "threads": {
          "type": "integer"
        },
        "wsize": {
          "type": "integer"
        },
        "rsize": {
          "type": "integer"
        },
        "pods_per_worker": {
          "type": "integer"
        },
        "scale_out_factor": {
          "type": "integer"
        },
        "mean": {
          "type": "float"
        },
        "min": {
          "type": "float"
        },
        "max": {
          "type": "float"
        },
        "stddev": {
          "type": "float"
        },
        "stddev_pct": {
          "type": "float"
        },
        "unit": {
          "type": "keyword"
        },
        "busy_cpu": {
          "type": "float"
        },
        "samples_count": {
          "type": "integer"
        },
        "regulus_git_branch": {
          "type": "keyword"
        },
        "execution_label": {
          "type": "keyword"
        }
      }
    }
  }
}
